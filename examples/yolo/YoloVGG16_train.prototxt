  top: "add_conv1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1024
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0.0
    }
  }
}
layer {
  name: "conv1_relu"  
  type: "ReLU"
  bottom: "add_conv1"
  top: "add_conv1"
  relu_param {
    negative_slope: 0.1  
  }
}
layer {
  name: "add_conv2"
  type: "Convolution"
  bottom: "add_conv1"
  top: "add_conv2"
  param {
