    kernel_size: 2
    stride: 2
  }
}

layer {
  name: "reg_reshape"
  type: "Reshape"
  bottom: "pool6"
  top: "regression"
  reshape_param {
    axis: 1
    shape {
      dim: 1470
    }
  }
}
layer {
  name: "det_loss"
  type: "DetectionLoss"
  bottom: "regression"
  bottom: "label"
  top: "det_loss"
  loss_weight: 1
  detection_loss_param {
    side: 7
    num_class: 20
    num_object: 2
    object_scale: 1.0
    noobject_scale: 0.5
    class_scale: 1.0
    coord_scale: 5.0
    sqrt: true
    constriant: true
  }
}

