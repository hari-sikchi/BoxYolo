name: "gnet_yolo"
layer {
  name: "data"
  type: "BoxData"
  top: "data"
  top: "label"
  include {
    phase: TRAIN  
  }
  transform_param {
    mirror: false
    force_color: true
    mean_value: 104
    mean_value: 117
    mean_value: 123
  }
  data_param {
    source: "/home/harshit/work/caffe-yolo/data/yolo/lmdb/trainval_lmdb"
    batch_size: 16
    side: 7
    backend: LMDB
  }
}
layer {
